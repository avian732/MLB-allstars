   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "batting = pd.read_csv('./Data/Batting.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "batting = batting[ (batting['yearID'] >=1987) & (batting['yearID'] <=2012)]\n",
    "batting = batting [ (batting['AB'] > 100) & (batting['G'] > 81)]\n",
    "batting = batting.drop(['stint', 'teamID', 'lgID', 'G_batting', 'G_old'], axis = 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "allstars = pd.read_csv('./Data/AllstarFull.csv')\n",
    "allstars = allstars[ (allstars['yearID'] >=1987) & (allstars['yearID'] <=2012)]\n",
    "allstars = allstars[['playerID', 'yearID', 'gameID']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "merged = pd.merge(batting, allstars, how = 'left', on = ['playerID', 'yearID'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "merged['All-Star'] = np.where(merged['gameID'].notnull() , True, False)"
   ]
  },
